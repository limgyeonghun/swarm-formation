Visualization Manager:
  Class: ""
  Displays:
      - Class: rviz_default_plugins/Marker  #(/path_markers)
        Enabled: true
        Name: Path and Obstacles
        Topic:
          Value: /path_markers
          Depth: 20
          History Policy: Keep Last
          Reliability Policy: Best Effort
          Durability Policy: Volatile
        Value: true
        Namespaces:
          path: true
          obstacle: true
        Alpha: 1.0
        Scale X: 0.1
        Scale Y: 0.1
        Scale Z: 0.1
      - Class: rviz_default_plugins/Marker  # (/opt_trajectory)
        Enabled: true
        Name: Min Jerk Trajectory
        Topic:
          Value: /opt_trajectory
          Depth: 20
          History Policy: Keep Last
          Reliability Policy: Best Effort
          Durability Policy: Volatile
        Value: true
        Alpha: 0.4
        Scale X: 0.05
        Scale Y: 0.05
        Scale Z: 0.05
        Color: 0;255;0 
        History Length: 1000

      - Class: rviz_default_plugins/Marker  # (/global_traj)
